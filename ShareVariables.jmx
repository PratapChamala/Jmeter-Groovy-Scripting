        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="02-Sample1-JSR223 Sampler">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">def JsToken = vars.get(&quot;TOKEN&quot;)

// Grab a token from JSON Extractor and save it to a variable 

log.info(&quot;Thread Group1 == JsToken=====&gt;&quot;+&quot;${JsToken}&quot;)

// to share varable b/w different two samplers with in the same thread group need to use perdefined variable - vars 

vars.put(&quot;PutVarsVarriable&quot;,JsToken)

def GetVarsVarriable= vars.get(&quot;PutVarsVarriable&quot;)

log.info(&quot;Thread Group1 == GetVarsVarriable=====&gt;&quot;+&quot;${GetVarsVarriable}&quot;)

// to share varable b/w different two Thread Groups need to use perdefined variable - props 

def PropsVariable = &quot;Perftest&quot;

props.put(&quot;PutPropsVarriable&quot;,PropsVariable)
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
